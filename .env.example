# Local LLM server base URL
DAYLE_SERVER=http://127.0.0.1:8081

# Logging format: text or json
LOG_FORMAT=json

# Auto reply for webhooks (1/true/yes)
AUTO_REPLY=1

# Twilio credentials
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_FROM=+15551230000

# Facebook Messenger
FB_VERIFY_TOKEN=verify_token_value
FB_APP_SECRET=app_secret_value
FB_PAGE_TOKEN=page_access_token

# Licensing
# Enforce licensing on protected endpoints in production
LICENSE_ENFORCE=0
# Base64-encoded issuer secret for offline activation tokens (do not commit real value)
LICENSE_ISSUER_SECRET=
